<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Colors Start -->
  <SolidColorBrush x:Key="BackgroundBrush1" Color="#FF3C3C3C"/>
  <SolidColorBrush x:Key="BackgroundBrush2" Color="#FF252526"/>
  <SolidColorBrush x:Key="BackgroundBrush3" Color="#FF1E1E1E"/>
  <Color x:Key="SelectedColor">#264F78</Color>
  <Color x:Key="DropShadowColor">White</Color>
  <SolidColorBrush x:Key="SelectedBrush" Color="#FF264F78"/>
  <SolidColorBrush x:Key="UnselectedBrush" Color="#FF3C3C3C"/>
  <SolidColorBrush x:Key="TextBrush" Color="#FFCCCCCC"/>
  <SolidColorBrush x:Key="TextBrushDim" Color="DarkGray"/>
  <SolidColorBrush x:Key="SplitterBrush" Color="#FF3C3C3C"/>
